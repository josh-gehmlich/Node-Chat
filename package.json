{
  "name": "Node-Chat",
  "version": "0.1.0",
  "description": "Demonstrate Websockets with Node.js",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node server/index.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "webpack-dev-server --config webpack.dev.js",
    "dev:server": "NODE_ENV=development nodemon server/index.js --watch server --watch .env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbizhev/Node-Chat.git"
  },
  "author": "Dolet Bizhev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dbizhev/Node-Chat/issues"
  },
  "homepage": "https://github.com/dbizhev/Node-Chat#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "morgan": "^1.9.0",
    "node-uuid": "^1.4.8",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.3.2",
    "ws": "^5.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "eslint": "^4.16.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.2.0",
    "imagemin-webpack-plugin": "^2.1.1",
    "lint-staged": "^6.1.0",
    "prettier": "^1.12.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
